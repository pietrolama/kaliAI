# KaliAI Environment Configuration
# Copia questo file in .env e inserisci i tuoi valori reali

# OpenAI/DeepSeek API Configuration
OPENAI_API_KEY=your_api_key_here
OPENAI_BASE_URL=https://api.deepseek.com/v1/

# Model Configuration
MODEL_NAME=deepseek-chat

# Docker Sandbox (true/false)
USE_DOCKER_SANDBOX=true

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=true
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
